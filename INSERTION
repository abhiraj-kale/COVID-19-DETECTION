EXECUTE SET_ZONES;

UPDATE SUSPECTS S
SET SUSPECT='Y' WHERE 
EXISTS ( SELECT 1 FROM ZONE_SELECT
WHERE ZONE='R' AND LOCATION=S.LOCATION AND S.USER_ID<>USER_ID
);

UPDATE SYMPTOMS
SET PERCENTAGE = CONCAT(PERCENTAGE,' %');
UPDATE ZONE_SELECT
SET PERCENTAGE = CONCAT(PERCENTAGE,' %');
