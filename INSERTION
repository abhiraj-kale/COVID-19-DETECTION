  CREATE SEQUENCE INCREMENT_SR
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE OR REPLACE TRIGGER TRIGGER1
BEFORE INSERT ON BASIC_INFO
FOR EACH ROW
  WHEN (new.SR_NO IS NULL)
BEGIN
  :new.SR_NO := INCREMENT_SR.NEXTVAL;
END;
/

